ifelse(`secondary genre` == "Lectionary", "Lectionary", "Other Service Book"))) %>%
# mutate(category = ifelse(`primary genre` == "Biblical","Biblical", "Non-Biblical")) %>%
filter(category != "Lectionary") %>%
group_by(century, category, form) %>%
summarise(count = n()/8) %>%
mutate(freq = count / sum(count)) %>%
ggplot(.) +
