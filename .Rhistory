count <- All_Variable_Letters_Tidy %>%
filter(letter != "alaph" & century < 1100 & century > 400 & (`primary genre` == "Biblical" | `primary genre` == "Theology" | `primary genre` == "Hagiographic" | `secondary genre` == "Lectionary" | (`primary genre` == "Service-Books" & `secondary genre` != "Lectionary"))) %>%
mutate(category = ifelse(`primary genre` != "Service-Books",
